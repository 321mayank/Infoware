<!DOCTYPE html>
<html>
  <head>
    <title>All Employees</title>
    <style>
      body {
        font-family: Arial, sans-serif;
        margin: 0;
        padding: 20px;
      }

      h1 {
        text-align: center;
        margin-bottom: 20px;
      }

      table {
        width: 100%;
        border-collapse: collapse;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
      }

      th,
      td {
        padding: 12px 15px;
        text-align: left;
        border-bottom: 1px solid #ddd;
      }

      th {
        background-color: #f2f2f2;
        font-weight: bold;
      }

      tr:hover {
        background-color: #f9f9f9;
      }
    </style>
  </head>
  <body>
    <h1>All Employees</h1>
    <table>
      <thead>
        <tr>
          <th>Emp ID</th>
          <th>Name</th>
          <th>Job</th>
          <th>Number</th>
          <th>Email</th>
          <th>Address</th>
          <th>City</th>
          <th>State</th>
          <th>Primary Emergency Contact</th>
          <th>Number</th>
          <th>Relationship</th>
          <th>Secondary Emergency Contact</th>
          <th>Number</th>
          <th>Relationship</th>
        </tr>
      </thead>
      <tbody>
        <% Employees.forEach(function(employee) { %>
          <tr>
            <td><%= employee.empID %></td>
            <td><%= employee.name %></td>
            <td><%= employee.job %></td>
            <td><%= employee.number %></td>
            <td><%= employee.email %></td>
            <td><%= employee.address %></td>
            <td><%= employee.city %></td>
            <td><%= employee.state %></td>
            <td><%= employee.primaryEmergencyContact %></td>
            <td><%= employee.PEnumber %></td>
            <td><%= employee.PErelationship %></td>
            <td><%= employee.secondaryEmergencyContact %></td>
            <td><%= employee.SEnumber %></td>
            <td><%= employee.SErelationship %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
